<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LoseResult">

    <TextView
        android:id="@+id/loseResultTitle"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:fontFamily="@font/poppins_light"
        android:text="To lose weight, you should eat:"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.050000012" />

    <TextView
        android:id="@+id/loseResultKcal"
        android:layout_width="173dp"
        android:layout_height="48dp"
        android:layout_marginTop="40dp"
        android:gravity="right"
        android:text="0000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loseResultTitle" />

    <TextView
        android:id="@+id/loseKcalTxt"
        android:layout_width="173dp"
        android:layout_height="48dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/poppins_light"
        android:text="Calories"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/loseResultKcal"
        app:layout_constraintTop_toBottomOf="@+id/loseResultTitle" />

    <TextView
        android:id="@+id/loseGramsOfProteins"
        android:layout_width="173dp"
        android:layout_height="48dp"
        android:layout_marginTop="40dp"
        android:gravity="right"
        android:text="0000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loseResultKcal" />

    <TextView
        android:id="@+id/loseProteinTxt"
        android:layout_width="173dp"
        android:layout_height="48dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/poppins_light"
        android:text="Grams of protein"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/loseGramsOfProteins"
        app:layout_constraintTop_toBottomOf="@+id/loseKcalTxt" />

    <TextView
        android:id="@+id/loseGramsOfFat"
        android:layout_width="173dp"
        android:layout_height="48dp"
        android:layout_marginTop="40dp"
        android:gravity="right"
        android:text="0000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loseGramsOfProteins" />

    <TextView
        android:id="@+id/loseFatTxt"
        android:layout_width="173dp"
        android:layout_height="48dp"
        android:layout_marginTop="40dp"
        android:fontFamily="@font/poppins_light"
        android:text="Grams of fat"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/loseGramsOfFat"
        app:layout_constraintTop_toBottomOf="@+id/loseProteinTxt" />

    <TextView
        android:id="@+id/loseGramsOfCarbs"
        android:layout_width="173dp"
        android:layout_height="48dp"
        android:layout_marginTop="40dp"
        android:gravity="right"
        android:text="0000"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loseGramsOfFat" />

    <TextView
        android:id="@+id/loseCarbsTxt"
        android:layout_width="173dp"
        android:layout_height="48dp"
        android:layout_marginTop="40dp"
        android:text="Grams of carbs"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/loseGramsOfCarbs"
        app:layout_constraintTop_toBottomOf="@+id/loseFatTxt" />

</androidx.constraintlayout.widget.ConstraintLayout>